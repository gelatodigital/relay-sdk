{
  "name": "@gelatonetwork/relay-sdk",
  "version": "3.5.0",
  "description": "SDK to integrate with Gelato Relay",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rm -rf dist && npx tsc",
    "format:check": "prettier --check \"*/**/*.{js,json,md,ts}\"",
